
I=$(HOME)/include
E=$(HOME)/bin
LIBS=$(HOME)/dbh/libbfs.a /lib/libfom.a  /lib/libuisam.a
OBJECTS=userprof.o
CC=cc

$E/userprof.out : $(OBJECTS) $(LIBS)
	$(CC) $(OBJECTS) $(LIBS) -o  $E/userprof.out

.c.o :
	$(CC) -c -I$I -DENGLISH $*.c

$(OBJECTS) :userprof.c $I/bfs_defs.h $I/bfs_recs.h /usr/include/cfomstrc.h 

