	cc -g -c -I/disk2/lbase/include -DENGLISH crt_yr.c
	cc -g crt_yr.o /disk2/lbase/dbh/libpp.a /lib/libfom.a  /lib/libuisam.a  -o /disk2/lbase/bin/crt_yr.out
