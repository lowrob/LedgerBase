	cc -g roll_att.o /disk2/lbase/dbh/libpp.a /lib/libfom.a  /lib/libuisam.a  -o /disk2/lbase/bin/roll_att.out
	cc -g -c -I/disk2/lbase/include -DENGLISH roll_sen.c
