
I=$(HOME)/include
E=$(HOME)/bin
LIBS=$(HOME)/dbh/libpp.a /lib/libfom.a  /lib/libuisam.a 
OBJECTS=resetgl.o
CC=cc -g

$E/resetgl.out : $(OBJECTS) $(LIBS)
	$(CC) $(OBJECTS) $(LIBS) -o $E/resetgl.out

.c.o :
	$(CC) -c -I$I -DENGLISH $*.c

$(OBJECTS) : $I/bfs_defs.h $I/bfs_pp.h /usr/include/cfomstrc.h

